{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "warn",
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "no-multiple-empty-lines": ["error", { "max": 2 }],
    "eol-last": ["error", "always"]
  },
  "globals": {
    "solana": "readonly",
    "phantom": "readonly",
    "Telegram": "readonly",
    "supabase": "readonly",
    "Chart": "readonly"
  }
}

















